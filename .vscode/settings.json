  "git.enableSmartCommit": true,
  "git.ignoreMissingGitWarning": true,
  "git.path": "/usr/bin/git",
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/tmp": true,
    "**/bower_components": true,
